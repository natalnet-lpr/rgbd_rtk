# - Config file for the rgbd_rtk package
# It defines the following variables
#  RGBD_RTK_INCLUDE_DIRS - include directories for rgbd_rtk
#  RGBD_RTK_LIBRARIES    - libraries to link against
#  RGBD_RTK_EXECUTABLE   - the executable

# Compute paths
get_filename_component(RGBD_RTK_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(RGBD_RTK_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET rgbd_rtk_common AND NOT rgbd_rtk_io AND NOT rgbd_rtk_quadtree
	AND NOT rgbd_rtk_tracking AND NOT rgbd_rtk_visualization AND NOT
	rgbd_rtk_motion_estimation AND NOT rgbd_rtk_stereo AND NOT
	rgbd_rtk_visual_odometry AND NOT rgbd_rtk_BINARY_DIR)
  include("${RGBD_RTK_CMAKE_DIR}/rgbd_rtkTargets.cmake")
endif()

# These are IMPORTED targets created by FooBarTargets.cmake
if(TARGET rgbd_rtk_slam)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_slam)
endif()
if(TARGET rgbd_rtk_common)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_common)
endif()
if(TARGET rgbd_rtk_io)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_io)
endif()
if(TARGET rgbd_rtk_quadtree)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_quadtree)
endif()
if(TARGET rgbd_rtk_tracking)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_tracking)
endif()
if(TARGET rgbd_rtk_visualization)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_visualization)
endif()
if(TARGET rgbd_rtk_motion_estimation)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_motion_estimation)
endif()
if(TARGET rgbd_rtk_stereo)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_stereo)
endif()
if(TARGET rgbd_rtk_visual_odometry)
	set(RGBD_RTK_LIBRARIES rgbd_rtk_visual_odometry)
endif()

set(RGBD_RTK_EXECUTABLE applications)
